{
  "name": "git-auto-deploy",
  "author": "Jefferson Dantas",
  "version": "1.0.0",
  "main": "src/index.ts",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "express-sse-middleware": "^3.0.3",
    "fs-extra": "^10.1.0",
    "google-auth-library": "^8.7.0",
    "nodemailer": "^6.8.0",
    "rimraf": "^3.0.2",
    "yaml": "^2.3.1",
    "yup": "^1.2.0",
    "yup-locale-pt": "^0.0.9"
  },
  "scripts": {
    "start": "node dist/server",
    "dev": "cross-env NODE_ENV=development ts-node -T --project tsconfig.server.json server/index.ts",
    "build": "rimraf .next dist out *.tsbuildinfo && tsc --project tsconfig.server.json && next build && next export",
    "check-types": "tsc --noEmit && tsc --project tsconfig.server.json --noEmit",
    "format": "prettier -w .",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@chakra-ui/react": "^2.7.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.9",
    "@types/nodemailer": "^6.4.8",
    "@types/react": "^18.2.12",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "axios": "^1.4.0",
    "eslint": "^8.43.0",
    "eslint-config-next": "^13.4.6",
    "eslint-config-prettier": "^8.8.0",
    "framer-motion": "^10.12.16",
    "husky": "^8.0.3",
    "next": "^13.4.6",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "react-sse-hooks": "^1.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
